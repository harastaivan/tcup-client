{
	"parser": "babel-eslint",
	"parserOptions": {
		"ecmaVersion": 6,
		"sourceType": "module",
		"ecmaFeatures": {
			"jsx": true,
			"modules": true,
			"experimentalObjectRestSpread": true
		}
	},
	"plugins": ["react"],
	"extends": ["eslint:recommended", "plugin:react/recommended"],
	"rules": {
		"strict": ["error", "never"]
	},
	"settings": {
		"react": {
			"pragma": "React",
			"version": "16.8.6"
		}
	},
	"env": { "browser": true, "node": true, "mocha": true }
}
